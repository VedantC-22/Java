		<property name="id" value="211" />
		<property name="address" >
			<value>Somatane phata, Pune</value>
		</property>
  	</bean>
  
  <!-- P schema example -->
  <bean id="student2" class="com.springcore.Student" p:id="212" p:name="Shivam" p:address="Earth" />
  
  <bean id="courses" class="com.springcore.Courses">
  <property name="coursesList">
      <list>
          <value>Java</value>
          <value>Python</value>
          <value>JavaScript</value>
          <value>HTML</value>
          <value>CSS</value>
        </list>
        </property>
  </bean>
  
